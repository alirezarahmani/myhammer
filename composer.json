{
  "name": "MyHammer/alireza-test",
  "type": "metapackage",
  "description": "My Hammer Development Environment",
  "homepage": "https://github.com/alirezarahmani/myhammer",
  "config": {
    "process-timeout": 2000
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "MyHammer\\": "src/",
      "Loader\\": "Bootstrap/",
      "": "web/"
    }
  },
  "require": {
    "mockery/mockery": "^1.1",
    "beberlei/assert": "^3.0",
    "symfony/dependency-injection": "^4.1",
    "symfony/yaml": "^4.1",
    "symfony/http-foundation": "^4.1",
    "symfony/event-dispatcher": "^4.1",
    "robmorgan/phinx": "^0.10.6",
    "symfony/routing": "^4.1"
  }
}
